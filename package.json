{
  "name": "react-component-example-project",
  "version": "0.1.0",
  "description": "A minimal boilerplate for building a react component for npm, including a demo page.",
  "main": "lib/index.js",
  "scripts": {
    "start": "concurrently \"npm run lib:watch\" \"npm run docs:watch\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib --watch --out-dir lib --copy-files",
    "docs": "webpack --mode production",
    "docs:watch": "webpack-dev-server --mode development"
  },
  "keywords": [],
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.3.0 || ^16.2.0",
    "react-dom": "^15.3.0 || ^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "babel-loader": "^8.0.0-beta.4",
    "concurrently": "^3.5.1",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "style-loader": "^0.222.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.3"
  },
  "author": "Markus Englund <markus.s.englund@gmail.com>",
  "homepage": "https://github.com/yogaboll/react-npm-component-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:yogaboll/react-npm-component-starter.git"
  }
}
